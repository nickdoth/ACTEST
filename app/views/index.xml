<Alloy>
	<Collection src="Highlights" />
	<Window>
		<TableView dataCollection="Highlights" onClick="tableOnClick">
		    <TableViewRow layout="vertical">
		        <ImageView image="{pic_url}" top="20dp" />
		        <Label text="{title}" bottom="20dp" /> 
		    </TableViewRow>
		</TableView>
	</Window>
</Alloy>